{% include 'config' %}
in:
  type: jdbc
  driver_path: {{driver_path}}
  driver_class: {{driver_class}}
  url: {{url}}
  user: {{userid}}
  password: {{password}}
  table: M_APCRD_MEM_TMP_MCH
  select: APCRD_NO
          ,MEM_ID
          ,MCH_FLG
          ,REG_DTTM
          ,REG_USR_ID
          ,UPD_DTTM
          ,UPD_USR_ID
          ,DAT_UPD_CLS
          ,UPD_TMS
          ,LGC_DEL_FLG
  where: {{where}}

{% comment %}
filters:
  - type: eval
    eval_columns:
      - APCRD_NO: value; print "ba noi cha may ", value, "\n";
      - MEM_ID: value
      - MCH_FLG: value
      - REG_DTTM: value
      - REG_USR_ID: value
      - UPD_DTTM: value
      - UPD_USR_ID: value
      - DAT_UPD_CLS: value
      - UPD_TMS: value
      - LGC_DEL_FLG: value
    out_columns:
      - APCRD_NO
      - MEM_ID
      - MCH_FLG
      - REG_DTTM
      - REG_USR_ID
      - UPD_DTTM
      - UPD_USR_ID
      - DAT_UPD_CLS
      - UPD_TMS
      - LGC_DEL_FLG
{% endcomment %}

out:
  type: stdout
