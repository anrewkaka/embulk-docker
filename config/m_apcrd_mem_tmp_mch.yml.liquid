{% include 'config' %}
in:
  type: jdbc
  driver_path: {{driver_path}}
  driver_class: {{driver_class}}
  url: {{url}}
  user: {{userid}}
  password: {{password}}
  table: M_APCRD_MEM_TMP_MCH
  select: APCRD_NO
          ,MEM_ID
          ,MCH_FLG
          ,REG_DTTM
          ,REG_USR_ID
          ,UPD_DTTM
          ,UPD_USR_ID
          ,DAT_UPD_CLS
          ,UPD_TMS
          ,LGC_DEL_FLG
  where: {{where}}

{% comment %}
filters:
  - type: eval
    eval_columns:
      - REG_DTTM: value
      - UPD_DTTM: value
      - UPD_TMS: 5
    out_columns:
      - REG_DTTM
      - UPD_DTTM
      - UPD_TMS
{% endcomment %}

type: stdout
